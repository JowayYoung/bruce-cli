{
	"name": "bruce-cli",
	"version": "1.1.3",
	"description": "A JS/React Application Automation Build Scaffold With Zero Configuration Out Of The Box",
	"keywords": [
		"bruce-app",
		"bruce-cli",
		"react-cli",
		"create-react",
		"create-react-app",
		"react",
		"webpack",
		"postcss",
		"scss",
		"less",
		"babel",
		"typescript",
		"stylelint",
		"eslint",
		"tslint",
		"build",
		"init",
		"locale",
		"new",
		"remove",
		"spa"
	],
	"author": {
		"name": "JowayYoung",
		"email": "young.joway@outlook.com"
	},
	"homepage": "https://github.com/JowayYoung",
	"repository": {
		"type": "git",
		"url": "https://github.com/JowayYoung/bruce-cli.git"
	},
	"bugs": {
		"email": "young.joway@outlook.com",
		"url": "https://github.com/JowayYoung/bruce-cli/issues"
	},
	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"bruce-app": "dist/index.js"
	},
	"scripts": {
		"build": "rimraf dist && tsc --project tsconfig.json && npm run copy && npm link",
		"copy": "ts-node-esm src/tasks/copy.ts",
		"dev": "nodemon --watch src --watch package.json --watch tsconfig.json --ext ts --exec \"npm run build\"",
		"init": "ts-node-esm src/tasks/convert.ts",
		"mount": "npm link"
	},
	"engines": {
		"node": ">=16.0.0",
		"npm": ">=7.10.0"
	},
