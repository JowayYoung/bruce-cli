	"type": "module",
	"main": "dist/index.js",
	"bin": {
		"bruce-app": "dist/index.js"
	},
	"scripts": {
		"build": "rimraf dist && ts-node-esm src/tasks/convert.ts && tsc -p tsconfig.json && ts-node-esm src/tasks/copy.ts",
		"dev": "nodemon -w src -w package.json -w tsconfig.json -e ts -x \"npm run build\""
	},
